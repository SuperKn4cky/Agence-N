{% extends 'base.html.twig' %}

{% block title %}Frais{% endblock %}

{% block body %}
    <div class="container">
    <table class="table table-gmk">
    <h1>Frais</h1>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ frai.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ frai.nom }}</td>
            </tr>
            <tr>
                <th>Service</th>
                <td>{{ frai.service }}</td>
            </tr>
            <tr>
                <th>Periode</th>
                <td>{{ frai.periode ? frai.periode|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ frai.status }}</td>
            </tr>
            <tr>
                <th>Montant Total de votre note de frais</th>
                <td>{{ frai.total }}</td>
            </tr>
        </tbody>
    </table>

    {{ include('note_frais/_delete_form.html.twig') }}
</div>
{% endblock %}
