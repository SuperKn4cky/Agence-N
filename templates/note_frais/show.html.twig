{% extends 'base.html.twig' %}

{% block title %}Frais{% endblock %}

{% block body %}
    <h1>Frais</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ frai.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ frai.nom }}</td>
            </tr>
            <tr>
                <th>Service</th>
                <td>{{ frai.service }}</td>
            </tr>
            <tr>
                <th>Periode</th>
                <td>{{ frai.periode ? frai.periode|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ frai.status }}</td>
            </tr>
            <tr>
                <th>Total</th>
                <td>{{ frai.total }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_note_frais_index') }}">Back to List</a>
    {{ include('note_frais/_delete_form.html.twig') }}
    <a href="{{ path('app_note_frais_edit', {'id': frai.id}) }}">Edit</a>

{% endblock %}
